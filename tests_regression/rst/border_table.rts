[STYLESHEET]
base = sphinx_base14

[VARIABLES]
mono_typeface=Courier
serif_typeface=Times
sans_typeface=Helvetica
thin_black_stroke=0.5pt,#000

[flowable with fat border : Flowable(has_class='fat-border')]
border = 20pt, #000
padding = 5pt
horizontal_align = center


[spacer : Paragraph(has_class='spacer')]
border = 1pt, #555
padding_top = 5.5cm
padding_bottom = 5.5cm
text_align = center
