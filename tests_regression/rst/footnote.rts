[STYLESHEET]
base = sphinx_base14

[VARIABLES]
mono_typeface=Courier
serif_typeface=Times
sans_typeface=Helvetica
thin_black_stroke=0.5pt,#000


[spacer1 : Paragraph(has_class='spacer1')]
border = 1pt, #555
padding_top = 7cm
padding_bottom = 7cm
text_align = center


[spacer2 : Paragraph(has_class='spacer2')]
border = 1pt, #555
padding_top = 7cm
padding_bottom = 6.6cm
text_align = center
